@model IEnumerable<WorkNetwork.Models.Persona>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombrePersona)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.apellidoPersona)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numeroDocumento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fechaNacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.correoElectronico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.domicilioPersona)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idLocalidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telefono1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telefono2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.estadoCivil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tituloAcademico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idSubRubro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cantidadHijos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombrePersona)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.apellidoPersona)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numeroDocumento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaNacimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.correoElectronico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.domicilioPersona)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idLocalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefono1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefono2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estadoCivil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tituloAcademico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idSubRubro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cantidadHijos)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idPersona">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idPersona">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idPersona">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
